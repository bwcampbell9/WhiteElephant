            android:id="@+id/action_mainSwipeView_to_postItemActivity"
            app:destination="@id/postItemActivity" />
    </fragment>
    <activity
        android:id="@+id/postItemActivity"
        android:name="com.example.white_elephant.PostItemActivity"
        android:label="activity_post_item"
        tools:layout="@layout/activity_post_item" />
